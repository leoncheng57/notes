public class Shapes {

    public String line(String thing, int freq){
	String ret = "";
	for (freq=freq;freq>0;freq=freq-1){
	    ret = ret + thing;
	}	
	return ret; 
    }
    
    public String box(int row, int col) {
	//row = 2; //testing
	String ret = "";
	
	for (int i = 0; i<row; i=i+1){
	    for (int j = 0; j<col; j=j+1){
		ret = ret+"*";
	    }
	    ret = ret+"\n";
	}
	return ret;
    }
    
    
    
}
